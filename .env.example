# Environment label
ENVIRONMENT=local

# OAuth2 password grant configuration for OpenEMR or similar systems
OAUTH_TOKEN_URL=https://openemr.example.com/oauth2/token
OAUTH_CLIENT_ID=demo-client-id
OAUTH_CLIENT_SECRET=demo-client-secret
OAUTH_USERNAME=demo-user
OAUTH_PASSWORD=demo-pass
OAUTH_SCOPE=patient.read
EXPIRES_SOON_SECONDS=120

# Patient Discovery downstream endpoint
PD_ENDPOINT_URL=https://mirth.example.com/patient-discovery
